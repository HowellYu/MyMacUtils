{
  "description": "Move cursor to between monitors",
  "manipulators": [
    {
      "description": "Left",
      "from": {
        "key_code": "j",
        "modifiers": {
          "mandatory": [
            "control"
          ]
        }
      },
      "to": [
        {
          "shell_command": "/opt/anaconda3/bin/python -c 'import Quartz.CoreGraphics as CG; CG.CGWarpMouseCursorPosition((1000, 500))'"
        }
      ],
      "type": "basic"
    },
    {
      "description": "Middle",
